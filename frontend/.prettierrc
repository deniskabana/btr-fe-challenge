{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 90,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.s?css",
      "options": {
        "parser": "stylelint"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    }
  ]
}
